{"ast":null,"code":"import { ref } from 'vue';\nimport LineChart from './LineChart.vue'; // 假设你有一个LineChart组件\n\nexport default {\n  name: 'MerchantDashboard',\n  components: {\n    LineChart\n  },\n  setup() {\n    const activeView = ref('orders');\n    const orders = ref([{\n      orderId: '001',\n      customerName: '张三',\n      orderAmount: 100,\n      orderDate: '2023-10-01'\n    }, {\n      orderId: '002',\n      customerName: '李四',\n      orderAmount: 200,\n      orderDate: '2023-10-02'\n    }\n    // 更多订单数据\n    ]);\n    const products = ref([{\n      productId: 'P001',\n      productName: '商品A',\n      productPrice: 50,\n      productStock: 100\n    }, {\n      productId: 'P002',\n      productName: '商品B',\n      productPrice: 150,\n      productStock: 200\n    }\n    // 更多商品数据\n    ]);\n    const salesData = ref([{\n      date: '2023-10-01',\n      amount: 100\n    }, {\n      date: '2023-10-02',\n      amount: 200\n    }\n    // 更多销售数据\n    ]);\n    const showOrders = () => {\n      activeView.value = 'orders';\n    };\n    const showProducts = () => {\n      activeView.value = 'products';\n    };\n    return {\n      activeView,\n      orders,\n      products,\n      salesData,\n      showOrders,\n      showProducts\n    };\n  }\n};","map":{"version":3,"names":["ref","LineChart","name","components","setup","activeView","orders","orderId","customerName","orderAmount","orderDate","products","productId","productName","productPrice","productStock","salesData","date","amount","showOrders","value","showProducts"],"sources":["E:\\Vue\\jsproject\\shop\\src\\views\\MerchantPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"merchant-dashboard\">\r\n      <el-container>\r\n        <!-- 侧边栏 -->\r\n        <el-aside width=\"200px\">\r\n          <el-menu default-active=\"1\" class=\"el-menu-vertical-demo\">\r\n            <el-menu-item index=\"1\" @click=\"showOrders\">\r\n              <i class=\"el-icon-s-order\"></i>\r\n              <span slot=\"title\">订单管理</span>\r\n            </el-menu-item>\r\n            <el-menu-item index=\"2\" @click=\"showProducts\">\r\n              <i class=\"el-icon-goods\"></i>\r\n              <span slot=\"title\">我的商品</span>\r\n            </el-menu-item>\r\n          </el-menu>\r\n        </el-aside>\r\n  \r\n        <!-- 主内容区 -->\r\n        <el-main>\r\n          <!-- 订单展示 -->\r\n          <div v-if=\"activeView === 'orders'\">\r\n            <h2>订单管理</h2>\r\n            <el-table :data=\"orders\" style=\"width: 100%\">\r\n              <el-table-column prop=\"orderId\" label=\"订单ID\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"customerName\" label=\"客户姓名\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"orderAmount\" label=\"订单金额\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"orderDate\" label=\"订单日期\" width=\"180\"></el-table-column>\r\n            </el-table>\r\n  \r\n            <!-- 七日内订单销售金额折线图 -->\r\n            <el-card class=\"box-card\">\r\n              <h3>七日内订单销售金额</h3>\r\n              <line-chart :data=\"salesData\"></line-chart>\r\n            </el-card>\r\n          </div>\r\n  \r\n          <!-- 商品管理 -->\r\n          <div v-if=\"activeView === 'products'\">\r\n            <h2>商品管理</h2>\r\n            <!-- 商品管理内容 -->\r\n            <el-table :data=\"products\" style=\"width: 100%\">\r\n              <el-table-column prop=\"productId\" label=\"商品ID\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"productName\" label=\"商品名称\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"productPrice\" label=\"商品价格\" width=\"180\"></el-table-column>\r\n              <el-table-column prop=\"productStock\" label=\"库存\" width=\"180\"></el-table-column>\r\n            </el-table>\r\n          </div>\r\n        </el-main>\r\n      </el-container>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { ref } from 'vue';\r\n  import LineChart from './LineChart.vue'; // 假设你有一个LineChart组件\r\n  \r\n  export default {\r\n    name: 'MerchantDashboard',\r\n    components: {\r\n      LineChart,\r\n    },\r\n    setup() {\r\n      const activeView = ref('orders');\r\n      const orders = ref([\r\n        { orderId: '001', customerName: '张三', orderAmount: 100, orderDate: '2023-10-01' },\r\n        { orderId: '002', customerName: '李四', orderAmount: 200, orderDate: '2023-10-02' },\r\n        // 更多订单数据\r\n      ]);\r\n  \r\n      const products = ref([\r\n        { productId: 'P001', productName: '商品A', productPrice: 50, productStock: 100 },\r\n        { productId: 'P002', productName: '商品B', productPrice: 150, productStock: 200 },\r\n        // 更多商品数据\r\n      ]);\r\n  \r\n      const salesData = ref([\r\n        { date: '2023-10-01', amount: 100 },\r\n        { date: '2023-10-02', amount: 200 },\r\n        // 更多销售数据\r\n      ]);\r\n  \r\n      const showOrders = () => {\r\n        activeView.value = 'orders';\r\n      };\r\n  \r\n      const showProducts = () => {\r\n        activeView.value = 'products';\r\n      };\r\n  \r\n      return {\r\n        activeView,\r\n        orders,\r\n        products,\r\n        salesData,\r\n        showOrders,\r\n        showProducts,\r\n      };\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .merchant-dashboard {\r\n    display: flex;\r\n    flex-direction: column;\r\n    height: 100vh;\r\n  }\r\n  \r\n  .el-container {\r\n    height: 100%;\r\n  }\r\n  \r\n  .el-aside {\r\n    background-color: #f5f5f5;\r\n  }\r\n  \r\n  .el-main {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .box-card {\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAqDE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,SAAQ,MAAO,iBAAiB,EAAE;;AAEzC,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIL,GAAG,CAAC,QAAQ,CAAC;IAChC,MAAMM,MAAK,GAAIN,GAAG,CAAC,CACjB;MAAEO,OAAO,EAAE,KAAK;MAAEC,YAAY,EAAE,IAAI;MAAEC,WAAW,EAAE,GAAG;MAAEC,SAAS,EAAE;IAAa,C