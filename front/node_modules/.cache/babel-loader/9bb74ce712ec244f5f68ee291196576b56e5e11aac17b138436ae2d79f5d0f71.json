{"ast":null,"code":"import { ref, onMounted, computed } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'CustomerPage',\n  setup() {\n    const products = ref([]);\n    const currentPage = ref(1);\n    const pageSize = ref(10);\n    const showDetails = ref(false);\n    const selectedProduct = ref({});\n    const fetchProducts = async () => {\n      try {\n        const response = await axios.get('/eShop/product/alllist');\n        if (response.data.code === 200) {\n          products.value = response.data.data;\n        } else {\n          console.error('Failed to fetch products:', response.data.msg);\n        }\n      } catch (error) {\n        console.error('Error fetching products:', error);\n      }\n    };\n    const paginatedProducts = computed(() => {\n      const start = (currentPage.value - 1) * pageSize.value;\n      const end = start + pageSize.value;\n      return products.value.slice(start, end);\n    });\n    const handlePageChange = page => {\n      currentPage.value = page;\n    };\n    const showProductDetails = product => {\n      selectedProduct.value = product;\n      showDetails.value = true;\n    };\n    const formatPath = path => path.replace(/\\\\/g, '/');\n    onMounted(() => {\n      fetchProducts();\n    });\n    return {\n      products,\n      currentPage,\n      pageSize,\n      paginatedProducts,\n      showDetails,\n      selectedProduct,\n      handlePageChange,\n      showProductDetails,\n      formatPath\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","name","setup","products","currentPage","pageSize","showDetails","selectedProduct","fetchProducts","response","get","data","code","value","console","error","msg","paginatedProducts","start","end","slice","handlePageChange","page","showProductDetails","product","formatPath","path","replace"],"sources":["E:\\Vue\\jsproject\\shop\\src\\views\\CustomerPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <img src=\"@/assets/image.png\" alt=\"Logo\" class=\"logo\" />\r\n    <div class=\"product-container\">\r\n      <div class=\"product-grid\">\r\n        <div v-for=\"product in paginatedProducts\" :key=\"product.id\" class=\"product-card\" @click=\"showProductDetails(product)\">\r\n          <img :src=\"formatPath(product.purl)\" alt=\"商品图片\" class=\"product-image\" />\r\n          <div class=\"product-info\">\r\n            <h3 class=\"product-name\">{{ product.pname }}</h3>\r\n            <hr class=\"divider\" />\r\n            <p class=\"product-price\">¥{{ product.ppri }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :total=\"products.length\"\r\n        :page-size=\"pageSize\"\r\n        @current-change=\"handlePageChange\"\r\n        class=\"pagination\"\r\n      ></el-pagination>\r\n\r\n      <el-dialog v-model=\"showDetails\" title=\"商品详情\">\r\n        <img :src=\"formatPath(selectedProduct.purl)\" alt=\"商品图片\" class=\"product-image-large\" />\r\n        <p>名称: {{ selectedProduct.pname }}</p>\r\n        <p>类型: {{ selectedProduct.ptype }}</p>\r\n        <p>描述: {{ selectedProduct.pdes }}</p>\r\n        <p>库存: {{ selectedProduct.pcount }}</p>\r\n        <p>价格: {{ selectedProduct.ppri }}</p>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, computed } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'CustomerPage',\r\n  setup() {\r\n    const products = ref([]);\r\n    const currentPage = ref(1);\r\n    const pageSize = ref(10);\r\n    const showDetails = ref(false);\r\n    const selectedProduct = ref({});\r\n\r\n    const fetchProducts = async () => {\r\n      try {\r\n        const response = await axios.get('/eShop/product/alllist');\r\n        if (response.data.code === 200) {\r\n          products.value = response.data.data;\r\n        } else {\r\n          console.error('Failed to fetch products:', response.data.msg);\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching products:', error);\r\n      }\r\n    };\r\n\r\n    const paginatedProducts = computed(() => {\r\n      const start = (currentPage.value - 1) * pageSize.value;\r\n      const end = start + pageSize.value;\r\n      return products.value.slice(start, end);\r\n    });\r\n\r\n    const handlePageChange = (page) => {\r\n      currentPage.value = page;\r\n    };\r\n\r\n    const showProductDetails = (product) => {\r\n      selectedProduct.value = product;\r\n      showDetails.value = true;\r\n    };\r\n\r\n    const formatPath = (path) => path.replace(/\\\\/g, '/');\r\n\r\n    onMounted(() => {\r\n      fetchProducts();\r\n    });\r\n\r\n    return {\r\n      products,\r\n      currentPage,\r\n      pageSize,\r\n      paginatedProducts,\r\n      showDetails,\r\n      selectedProduct,\r\n      handlePageChange,\r\n      showProductDetails,\r\n      formatPath,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.app-container {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  background: linear-gradient(to bottom, #b3e5fc, #f8bbd0);\r\n}\r\n\r\n.logo {\r\n  position: absolute;\r\n  top: 20px;\r\n  left: 20px;\r\n  width: 100px; /* 调整大小 */\r\n  height: auto;\r\n}\r\n\r\n.product-container {\r\n  padding: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.product-grid {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  justify-content: center;\r\n  width: 100%;\r\n  padding: 20px;\r\n}\r\n\r\n.product-card {\r\n  width: 200px;\r\n  border: 1px solid #e0e0e0;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  transition: transform 0.2s;\r\n  background-color: white;\r\n}\r\n\r\n.product-card:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.product-image {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n\r\n.product-info {\r\n  padding: 10px;\r\n  text-align: left;\r\n}\r\n\r\n.divider {\r\n  margin: 10px 0;\r\n  border: none;\r\n  border-top: 1px solid #e0e0e0;\r\n}\r\n\r\n.product-name {\r\n  font-size: 16px;\r\n  margin: 0;\r\n}\r\n\r\n.product-price {\r\n  color: #ff5722;\r\n  font-size: 14px;\r\n  margin: 5px 0 0;\r\n}\r\n\r\n.product-image-large {\r\n  width: 100%;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.pagination {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.el-dialog {\r\n  .el-dialog__body {\r\n    padding: 20px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAqCA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAC9C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMO,WAAU,GAAIP,GAAG,CAAC,CAAC,CAAC;IAC1B,MAAMQ,QAAO,GAAIR,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMS,WAAU,GAAIT,GAAG,CAAC,KAAK,CAAC;IAC9B,MAAMU,eAAc,GAAIV,GAAG,CAAC,CAAC,CAAC,CAAC;IAE/B,MAAMW,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,wBAAwB,CAAC;QAC1D,IAAID,QAAQ,CAACE,IAAI,CAACC,IAAG,KAAM,GAAG,EAAE;UAC9BT,QAAQ,CAACU,KAAI,GAAIJ,QAAQ,CAACE,IAAI,CAACA,IAAI;QACrC,OAAO;UACLG,OAAO,CAACC,KAAK,CAAC,2BAA2B,EAAEN,QAAQ,CAACE,IAAI,CAACK,GAAG,CAAC;QAC/D;MACF,EAAE,OAAOD,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IAED,MAAME,iBAAgB,GAAIlB,QAAQ,CAAC,MAAM;MACvC,MAAMmB,KAAI,GAAI,CAACd,WAAW,CAACS,KAAI,GAAI,CAAC,IAAIR,QAAQ,CAACQ,KAAK;MACtD,MAAMM,GAAE,GAAID,KAAI,GAAIb,QAAQ,CAACQ,KAAK;MAClC,OAAOV,QAAQ,CAACU,KAAK,CAACO,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IACzC,CAAC,CAAC;IAEF,MAAME,gBAAe,GAAKC,IAAI,IAAK;MACjClB,WAAW,CAACS,KAAI,GAAIS,IAAI;IAC1B,CAAC;IAED,MAAMC,kBAAiB,GAAKC,OAAO,IAAK;MACtCjB,eAAe,CAACM,KAAI,GAAIW,OAAO;MAC/BlB,WAAW,CAACO,KAAI,GAAI,IAAI;IAC1B,CAAC;IAED,MAAMY,UAAS,GAAKC,IAAI,IAAKA,IAAI,CAACC,OAAO,CAAC,KAAK,EAAE,GAAG,CAAC;IAErD7B,SAAS,CAAC,MAAM;MACdU,aAAa,CAAC,CAAC;IACjB,CAAC,CAAC;IAEF,OAAO;MACLL,QAAQ;MACRC,WAAW;MACXC,